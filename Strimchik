   "source": [
    "display(game)\n",
    "\n",
    "def top_platform(name_platform):\n",
    "    platform = game[game['platform'] == name_platform]\n",
    "    fig, ax = plt.subplots(1 ,2, figsize=(15,5))\n",
    "    sns.scatterplot(x='user_score', y='all_sales', data=platform, ax=ax[0])\n",
    "    sns.scatterplot(x='critic_score', y='all_sales', data=platform, ax=ax[1])\n",
    "    fig.suptitle(name_platform, fontsize=15)\n",
    "    ax[0].set(xlabel='Оценки пользователей')\n",
    "    ax[1].set(xlabel='Оценки критиков')\n",
    "    ax[0].set(ylabel='Количество продаж')\n",
    "    ax[1].set(ylabel='Количество продаж')\n",
    "    plt.show()\n",
    "    \n",
    "    cor = platform['user_score'].corr(platform['all_sales'])\n",
    "    critic_cor = platform['critic_score'].corr(platform['all_sales'])\n",
    "\n",
    "    \n",
    "    if 0.3 >= critic_cor >= 0.1:\n",
    "        print('Корреляция между отзывами критиков и игровой платформой ', name_platform.upper(), ': Слабая', critic_cor)\n",
    "    if 0.5 >= critic_cor >= 0.3:\n",
    "        print('Корреляция между отзывами критиков и игровой платформой ', name_platform.upper(), ': Средняя', critic_cor)\n",
    "    if 0.7 >= critic_cor >= 0.5:\n",
    "        print('Корреляция между отзывами критиков и игровой платформой ', name_platform.upper(), ': Высокая', critic_cor)\n",
    "    if 0.9 >= critic_cor >= 0.7:\n",
    "        print('Корреляция между отзывами критиков и игровой платформой ', name_platform.upper(), ': Достаточно высокая', critic_cor)\n",
    "    if 1 >= critic_cor >= 0.9:\n",
    "        print('Корреляция между отзывами критиков и игровой платформой ', name_platform.upper(), ': Сильная', critic_cor)\n",
    "    \n",
    "        \n",
    "    if 0.3 >= cor >= 0.1:\n",
    "        print('Корреляция между отзывами пользователей и продажами ', name_platform.upper(), ': Слабая', cor)\n",
    "    if 0.5 >= cor >= 0.3:\n",
    "        print('Корреляция между отзывами пользователей и продажами ', name_platform.upper(), ': Средняя', cor)\n",
    "    if 0.7 >= cor >= 0.5:\n",
    "        print('Корреляция между отзывами пользователей и продажами ', name_platform.upper(), ': Высокая', cor)\n",
    "    if 0.9 >= cor >= 0.7:\n",
    "        print('Корреляция между отзывами пользователей и продажами ', name_platform.upper(), ': Достаточно высокая', cor)\n",
    "    if 1 >= cor >= 0.9:\n",
    "        print('Корреляция между отзывами пользователей и продажами ', name_platform.upper(), ': Сильная', cor)\n",
    "\n",
    "  \n",
    "    print('\\n')"
   ]
  },
  {
   "cell_type": "markdown",
